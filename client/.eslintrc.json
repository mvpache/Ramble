{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "class-methods-use-this": 0,
    "no-shadow": [
      "error",
      {
        "builtinGlobals": false,
        "hoist": "functions",
        "allow": ["err", "username", "response"]
      }
    ]
  }
}
